<p id="notice"><%= notice %></p>

<h1>Communities</h1>

<table class="table">
  <thead>
    <tr>
      <th class="table-header"> Name</th>
      <th class="table-header" >Address</th>
      <th class="table-header" colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @communities.each do |community| %>
      <tr>
        <td><%= link_to community.name, community_path(community) %></td>
        <td><%= community.address %></td>
        <td> <iframe
          width="200" height="150" frameborder="0" style="border:0"
               src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJtYX1Xjn2wokR7Mj2eOaubCw&key=AIzaSyDz83RWp8N_hm3hBLQOg__wSCDWR4_rt08" allowfullscreen></iframe>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community', new_community_path %>
